# Ignore bundler config.
mtma_be/.bundle

# Ignore all environment files.
mtma_be/.env*

# Ignore all logfiles and tempfiles.
mtma_be/log/*
mtma_be/tmp/*
!mtma_be/log/.keep
!mtma_be/tmp/.keep

# Ignore pidfiles, but keep the directory.
mtma_be/tmp/pids/*
!mtma_be/tmp/pids/
!mtma_be/tmp/pids/.keep

# Ignore storage (uploaded files in development and any SQLite databases).
mtma_be/storage/*
!mtma_be/storage/.keep
mtma_be/tmp/storage/*
!mtma_be/tmp/storage/
!mtma_be/tmp/storage/.keep

# Ignore key files for decrypting credentials and more.
mtma_be/config/*.key


